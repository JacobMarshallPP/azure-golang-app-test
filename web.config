<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="httpplatformhandler"
           path="\*"
           verb="\*"
           modules="httpPlatformHandler"
           resourceType="Unspecified" />
    </handlers>
    <httpPlatform processPath="d:\home\site\wwwroot\lib\go\bin\go.exe" 
                  arguments="run d:\home\site\wwwroot\*.go" 
                  startupTimeLimit="60"
                  startupRetryCount="10"
                  stdoutLogEnabled="true">
      <environmentVariables>
        <environmentVariable name="GOROOT"
                             value="d:\home\site\wwwroot\lib\go" />
      </environmentVariables>
    </httpPlatform>
  </system.webServer>
</configuration>